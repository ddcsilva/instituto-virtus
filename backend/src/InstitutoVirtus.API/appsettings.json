{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=virtusdb;Username=postgres;Password=postgres"
  },
  "DatabaseProvider": "PostgreSQL",
  "EnableSensitiveDataLogging": false,
  "JwtSettings": {
    "SecretKey": "SuaChaveSecretaMuitoSeguraComPeloMenos256Bits!@#$%",
    "Issuer": "InstitutoVirtus",
    "Audience": "InstitutoVirtusUsers"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://app.institutovirtus.com.br"
    ]
  },
  "Storage": {
    "BasePath": "C:\\VirtusStorage"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
